extend type Query {
    allUnits: [Unit!]!
}
extend type Mutation {
    createUnit(input: CreateUnitInput!): CreateUnitPayload!
}

type Unit {
    id: ID!
    longName: String!
    shortName: String!
}

input CreateUnitInput {
    longName: String!
    shortName: String!
}

type CreateUnitPayload {
    unit: Unit!
}

